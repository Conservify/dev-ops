{
    "template": "graylog_*",
    "mappings" : {
        "message" : {
            "properties" : {
                "service_trace" : {
                    "type" : "string",
                    "analyzer" : "whitespace",
                    "index" : "analyzed"
                },
                "error" : {
                    "type" : "string",
                    "analyzer" : "whitespace",
                    "index" : "analyzed"
                }
            }
        }
    }
}

